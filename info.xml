<?xml version="1.0"?>
<extension key="contactcreationinfo" type="module">
  <file>contactcreationinfo</file>
  <name>contactcreationinfo</name>
  <description>A simple CiviCRM extension that saves the current user and time to custom fields when creating a new contact.</description>
  <license>MIT</license>
  <maintainer>
    <author>Jason Davies</author>
    <email>jason@jasontdc.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/jasontdc/contactcreationinfo</url>
    <url desc="Documentation">https://github.com/jasontdc/contactcreationinfo</url>
    <url desc="Support">https://github.com/jasontdc/contactcreationinfo</url>
    <url desc="Licensing">http://opensource.org/licenses/MIT</url>
  </urls>
  <releaseDate>2023-04-04</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Contactcreationinfo</namespace>
    <format>22.10.0</format>
    <angularModule>crmContactcreationinfo</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
